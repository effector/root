{
  "name": "effector-root",
  "description": "Units in the root domain for testing and SSR support",
  "version": "1.3.0",
  "main": "src/common.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": ["src", "compat.js", "compat.d.ts", "macro.js", "macro.d.ts"],
  "exports": {
    ".": {
      "require": "./src/common.js",
      "default": "./src/index.js"
    },
    "./src/common.js": {
      "require": "./src/common.js",
      "default": "./src/index.js"
    },
    "./src/common": {
      "require": "./src/common.js",
      "default": "./src/index.js"
    },
    "./src/index.js": {
      "require": "./src/common.js",
      "default": "./src/index.js"
    },
    "./src/index": {
      "require": "./src/common.js",
      "default": "./src/index.js"
    },
    "./src": {
      "require": "./src/common.js",
      "default": "./src/index.js"
    },
    "./compat": "./compat.js"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint --ext .ts,.tsx ./",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/effector/root.git"
  },
  "keywords": ["effector", "root", "domain"],
  "author": "Sergey Sova <mail@sergeysova.com> (https://sergeysova.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/effector/root/issues"
  },
  "homepage": "https://github.com/effector/root#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-tester": "^10.0.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "effector": "^21.8.7",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "husky": "3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.1.3",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "effector": "^21.8.7 || ^22.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
